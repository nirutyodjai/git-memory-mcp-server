{
  "dashboard": {
    "id": null,
    "title": "Git Memory MCP Server - Production Dashboard",
    "tags": ["git-memory", "production", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*"
        },
        {
          "name": "job",
          "type": "query",
          "query": "label_values(up, job)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "deployment_timestamp",
          "iconColor": "green",
          "titleFormat": "Deployment",
          "textFormat": "Version: {{version}}"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=~\"$job\", instance=~\"$instance\"}",
            "legendFormat": "{{job}} - {{instance}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"options": {"0": {"text": "DOWN"}, "1": {"text": "UP"}}}
            ]
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "HTTP Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "rate(http_requests_total{job=~\"$job\", instance=~\"$instance\"}[5m])",
            "legendFormat": "{{method}} {{status}} - {{instance}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Requests/sec", "min": 0},
          {"show": false}
        ],
        "xAxis": {"show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 3,
        "title": "Response Time (95th percentile)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=~\"$job\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "95th percentile - {{instance}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=~\"$job\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "50th percentile - {{instance}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Seconds", "min": 0},
          {"show": false}
        ],
        "xAxis": {"show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"4..|5..\", job=~\"$job\", instance=~\"$instance\"}[5m]) / rate(http_requests_total{job=~\"$job\", instance=~\"$instance\"}[5m]) * 100",
            "legendFormat": "Error Rate % - {{instance}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Percentage", "min": 0, "max": 100},
          {"show": false}
        ],
        "xAxis": {"show": true},
        "legend": {"show": true, "values": true, "current": true},
        "alert": {
          "conditions": [
            {
              "query": {"queryType": "", "refId": "A"},
              "reducer": {"type": "last", "params": []},
              "evaluator": {"params": [5], "type": "gt"}
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data"
        }
      },
      {
        "id": 5,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "100 - (avg by(instance) (irate(node_cpu_seconds_total{mode=\"idle\", instance=~\"$instance\"}[5m])) * 100)",
            "legendFormat": "CPU Usage % - {{instance}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Percentage", "min": 0, "max": 100},
          {"show": false}
        ],
        "xAxis": {"show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 6,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "(node_memory_MemTotal_bytes{instance=~\"$instance\"} - node_memory_MemAvailable_bytes{instance=~\"$instance\"}) / node_memory_MemTotal_bytes{instance=~\"$instance\"} * 100",
            "legendFormat": "Memory Usage % - {{instance}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Percentage", "min": 0, "max": 100},
          {"show": false}
        ],
        "xAxis": {"show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 7,
        "title": "Disk Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "(node_filesystem_size_bytes{instance=~\"$instance\", mountpoint=\"/\"} - node_filesystem_free_bytes{instance=~\"$instance\", mountpoint=\"/\"}) / node_filesystem_size_bytes{instance=~\"$instance\", mountpoint=\"/\"} * 100",
            "legendFormat": "Disk Usage % - {{instance}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Percentage", "min": 0, "max": 100},
          {"show": false}
        ],
        "xAxis": {"show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 8,
        "title": "Database Connections",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "pg_stat_database_numbackends{instance=~\"$instance\"}",
            "legendFormat": "Active Connections - {{datname}}",
            "refId": "A"
          },
          {
            "expr": "pg_settings_max_connections{instance=~\"$instance\"}",
            "legendFormat": "Max Connections",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Connections", "min": 0},
          {"show": false}
        ],
        "xAxis": {"show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 9,
        "title": "Redis Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "redis_memory_used_bytes{instance=~\"$instance\"}",
            "legendFormat": "Used Memory - {{instance}}",
            "refId": "A"
          },
          {
            "expr": "redis_memory_max_bytes{instance=~\"$instance\"}",
            "legendFormat": "Max Memory - {{instance}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Bytes", "min": 0},
          {"show": false}
        ],
        "xAxis": {"show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 10,
        "title": "Business Metrics - User Registrations",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "rate(user_registrations_total{instance=~\"$instance\"}[1h])",
            "legendFormat": "Registrations/hour - {{instance}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Registrations/hour", "min": 0},
          {"show": false}
        ],
        "xAxis": {"show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 11,
        "title": "Business Metrics - Payment Success Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36},
        "targets": [
          {
            "expr": "rate(payment_transactions_total{status=\"success\", instance=~\"$instance\"}[10m]) / rate(payment_transactions_total{instance=~\"$instance\"}[10m]) * 100",
            "legendFormat": "Success Rate % - {{instance}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Percentage", "min": 0, "max": 100},
          {"show": false}
        ],
        "xAxis": {"show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 12,
        "title": "Active Subscriptions by Tier",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 44},
        "targets": [
          {
            "expr": "subscription_active_total{instance=~\"$instance\"}",
            "legendFormat": "{{tier}}",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "visible", "placement": "right"}
        }
      },
      {
        "id": 13,
        "title": "Monthly Recurring Revenue (MRR)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 44},
        "targets": [
          {
            "expr": "sum(subscription_mrr_total{instance=~\"$instance\"})",
            "legendFormat": "Total MRR",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 10000},
                {"color": "green", "value": 50000}
              ]
            },
            "unit": "currencyUSD"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 14,
        "title": "API Endpoint Performance",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 52},
        "targets": [
          {
            "expr": "topk(10, avg by (method, handler) (rate(http_request_duration_seconds_sum{instance=~\"$instance\"}[5m]) / rate(http_request_duration_seconds_count{instance=~\"$instance\"}[5m])))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {},
              "renameByName": {
                "method": "Method",
                "handler": "Endpoint",
                "Value": "Avg Response Time (s)"
              }
            }
          }
        ],
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        }
      },
      {
        "id": 15,
        "title": "Security Events",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 60},
        "targets": [
          {
            "expr": "rate(auth_failed_attempts_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "Failed Login Attempts/sec - {{instance}}",
            "refId": "A"
          },
          {
            "expr": "rate(api_rate_limit_exceeded_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "Rate Limit Exceeded/sec - {{instance}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Events/sec", "min": 0},
          {"show": false}
        ],
        "xAxis": {"show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 16,
        "title": "Container Resource Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 60},
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=~\"git-memory.*\"}[5m]) * 100",
            "legendFormat": "CPU % - {{name}}",
            "refId": "A"
          },
          {
            "expr": "container_memory_usage_bytes{name=~\"git-memory.*\"} / 1024 / 1024",
            "legendFormat": "Memory MB - {{name}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "CPU % / Memory MB", "min": 0},
          {"show": false}
        ],
        "xAxis": {"show": true},
        "legend": {"show": true, "values": true, "current": true}
      }
    ]
  },
  "overwrite": true
}