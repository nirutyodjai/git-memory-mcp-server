      }
    },
    "clickup-mcp": {
      "transportType": "stdio",
      "command": "npx",
      "args": ["-y", "@taazkareem/clickup-mcp-server@latest"],
      "env": {
        "CLICKUP_API_KEY": "test-key",
        "CLICKUP_TEAM_ID": "test-team"
      },
      "options": {
        "logEnabled": true
      }
    }
  }
}