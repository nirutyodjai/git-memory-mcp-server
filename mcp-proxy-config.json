{
  "mcpProxy": {
    "baseURL": "http://localhost:9090",
    "addr": ":9090",
    "name": "MCP Proxy Server",
    "version": "1.0.0",
    "type": "sse",
    "options": {
      "panicIfInvalid": false,
      "logEnabled": true,
      "authTokens": []
    }
  },
  "mcpServers": {
    "shadcn-ui": {
      "transportType": "stdio",
      "command": "npx",
      "args": ["@jpisnice/shadcn-ui-mcp-server"],
      "env": {},
      "options": {
        "logEnabled": true
      }
    },
    "magic-ui": {
      "transportType": "stdio",
      "command": "npx",
      "args": ["-y", "@magicuidesign/mcp@latest"],
      "env": {},
      "options": {
        "logEnabled": true
      }
    },
    "google-workspace": {
      "transportType": "stdio",
      "command": "uvx",
      "args": ["workspace-mcp", "--tool-tier", "core"],
      "env": {
        "GOOGLE_OAUTH_CLIENT_ID": "${GOOGLE_OAUTH_CLIENT_ID}",
        "GOOGLE_OAUTH_CLIENT_SECRET": "${GOOGLE_OAUTH_CLIENT_SECRET}",
        "OAUTHLIB_INSECURE_TRANSPORT": "1"
      },
      "options": {
        "logEnabled": true
      }
    },
    "playwright": {
      "transportType": "stdio",
      "command": "node",
      "args": ["src/playwright/index.ts"],
      "env": {},
      "options": {
        "logEnabled": true
      }
    },
    "multi-fetch": {
      "transportType": "stdio",
      "command": "python",
      "args": ["-m", "src.fetch"],
      "env": {},
      "options": {
        "logEnabled": true
      }
    },
    "blender": {
      "transportType": "stdio",
      "command": "node",
      "args": ["src/blender/index.ts"],
      "env": {},
      "options": {
        "logEnabled": true
      }
    },
    "memory": {
      "transportType": "stdio",
      "command": "node",
      "args": ["src/memory/index.ts"],
      "env": {},
      "options": {
        "logEnabled": true
      }
    },
    "sequential-thinking": {
      "transportType": "stdio",
      "command": "node",
      "args": ["src/sequentialthinking/index.ts"],
      "env": {},
      "options": {
        "logEnabled": true
      }
    },

    "oneuptime-mcp": {
      "transportType": "stdio",
      "command": "npx",
      "args": ["-y", "@oneuptime/mcp-server"],
      "env": {},
      "options": {
        "logEnabled": true
      }
    },
    "supabase": {
      "transportType": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-supabase"],
      "env": {
        "SUPABASE_URL": "https://rnuvnufywjvztbbnrpyc.supabase.co",
        "SUPABASE_ANON_KEY": "sb_publishable_W7u9fyItxkqtu7AoNX6dZA_JfZyDhmH"
      },
      "options": {
        "logEnabled": true
      }
    },
    "mongodb": {
      "transportType": "stdio",
      "command": "npx",
      "args": ["-y", "mongodb-mcp-server", "--readOnly"],
      "env": {
        "MDB_MCP_CONNECTION_STRING": "mongodb://localhost:27017/myDatabase",
        "MDB_MCP_READ_ONLY": "true"
      },
      "options": {
        "logEnabled": true
      }
    },
    "mongo-mcp": {
      "transportType": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-mongo-server"],
      "env": {
        "MCP_MONGODB_URI": "mongodb://localhost:27017/database",
        "MCP_MONGODB_READONLY": "true"
      },
      "options": {
        "logEnabled": true
      }
    },
    "gitlab": {
      "transportType": "stdio",
      "command": "npx",
      "args": ["-y", "@zereight/mcp-gitlab"],
      "env": {
        "GITLAB_PERSONAL_ACCESS_TOKEN": "YOUR_GITLAB_TOKEN_HERE",
        "GITLAB_API_URL": "https://gitlab.com/api/v4"
      },
      "options": {
        "logEnabled": true
      }
    },
    "brave-search": {
       "transportType": "stdio",
       "command": "npx",
