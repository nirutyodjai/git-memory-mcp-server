{
  "name": "nexus-ide-main-system",
  "version": "2.0.0",
  "description": "NEXUS IDE - The Ultimate AI-Powered Development Environment with Git Memory MCP Server",
  "main": "start-nexus-ide.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "bin": {
    "nexus-ide": "./bin/nexus-ide.js",
    "nexus": "./bin/nexus.js",
    "git-memory": "./bin/git-memory.js",
    "mcp-server": "./bin/mcp-server.js"
  },
  "files": [
    "src/",
    "bin/",
    "docs/",
    "config/",
    "templates/",
    "start-nexus-ide.js",
    "nexus-ide-integration.js",
    "nexus-mcp-server.js",
    "nexus-config.json",
    "ecosystem.config.js",
    "docker-compose.yml",
    "Dockerfile",
    "NEXUS-IDE-INTEGRATION.md",
    "DEPLOYMENT-GUIDE.md"
  ],
  "scripts": {
    "start": "node start-nexus-ide.js",
    "dev": "NODE_ENV=development DEBUG=nexus:* node start-nexus-ide.js",
    "build": "npm run build:typescript && npm run build:frontend",
    "build:typescript": "tsc --build",
    "build:frontend": "webpack --mode production",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "lint": "eslint src/ --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint src/ --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write src/",
    "analyze": "node src/tools/code-analyzer.js",
    "organize": "node src/tools/file-organizer.js",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "clean": "rimraf dist/ build/ coverage/ logs/ .nyc_output/",
    "setup": "npm install && npm run build && npm run setup:config",
    "setup:config": "node scripts/setup-config.js",
    "prd": "node src/tools/prd-tracker.js",
    "nexus:start": "node start-nexus-ide.js",
    "nexus:start:debug": "DEBUG=nexus:* node start-nexus-ide.js --debug",
    "nexus:start:prod": "NODE_ENV=production node start-nexus-ide.js --production",
    "nexus:status": "node scripts/check-status.js",
    "nexus:config": "node scripts/manage-config.js",
    "nexus:init": "node nexus-ide-integration.js",
    "nexus:mcp": "node nexus-mcp-server.js",
    "nexus:demo": "node nexus-ide-integration.js --demo",
    "nexus:update": "git pull && npm install && npm run build && npm restart",
    "nexus:backup": "node scripts/backup-system.js",
    "nexus:restore": "node scripts/restore-system.js",
    "nexus:monitor": "node scripts/system-monitor.js",
    "nexus:benchmark": "node scripts/performance-benchmark.js",
    "docker:build": "docker build -t nexus-ide .",
    "docker:run": "docker run -p 3000:3000 -p 3001:3001 nexus-ide",
    "docker:compose": "docker-compose up -d",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop nexus-ide",
    "pm2:restart": "pm2 restart nexus-ide",
    "pm2:logs": "pm2 logs nexus-ide",
    "pm2:monit": "pm2 monit",
    "security:audit": "npm audit && node scripts/security-check.js",
    "security:fix": "npm audit fix",
    "performance:profile": "node --prof start-nexus-ide.js",
    "performance:analyze": "node --prof-process isolate-*.log > profile.txt",
    "docs:generate": "jsdoc src/ -r -d docs/",
    "docs:serve": "http-server docs/ -p 8080",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "postinstall": "node scripts/post-install.js"
  },
  "keywords": [
    "nexus-ide",
    "ai-powered-ide",
    "git-memory",
    "mcp-server",
    "development-environment",
    "ai-assistant",
    "code-completion",
    "real-time-collaboration",
    "intelligent-memory",
    "git-integration",
    "websocket",
    "api-gateway",
    "performance-optimization",
    "security",
    "scalable",
    "typescript",
    "javascript",
    "nodejs",
    "ai",
    "machine-learning",
    "developer-tools",
    "productivity",
    "collaboration",
    "version-control",
    "code-analysis"
  ],
  "author": {
    "name": "NEXUS IDE Team",
    "email": "team@nexus-ide.com",
    "url": "https://nexus-ide.com"
  },
  "contributors": [
    {
      "name": "AI Development Team",
      "email": "ai-team@nexus-ide.com"
    },
    {
      "name": "Backend Team",
      "email": "backend-team@nexus-ide.com"
    },
    {
      "name": "Frontend Team",
      "email": "frontend-team@nexus-ide.com"
    }
  ],
  "license": "MIT",
  "homepage": "https://nexus-ide.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/nexus-ide/git-memory-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/nexus-ide/git-memory-mcp-server/issues",
    "email": "bugs@nexus-ide.com"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/nexus-ide"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@types/node": "^20.10.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "pm2": "^5.3.0",
    "redis": "^4.6.11",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.2",
    "playwright": "^1.40.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/*.(test|spec).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!src/**/*.d.ts",
      "!src/tests/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "config": {
    "port": 3000,
    "wsPort": 3001,
    "logLevel": "info",
    "maxMemory": "2GB",
    "clustering": true,
    "ssl": false
  }
}