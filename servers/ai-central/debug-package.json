    "start": "node start-debug-assistant.js",
    "dev": "nodemon start-debug-assistant.js",
    "debug": "node --inspect start-debug-assistant.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "build": "echo 'No build step required'",
    "health": "curl -f http://localhost:3010/health || exit 1",
    "logs": "tail -f logs/debug-assistant.log",
    "clean": "rm -rf logs/* temp/* data/debug-logs/*",
    "benchmark": "node benchmark/debug-performance.js",
    "docs": "jsdoc -c jsdoc.conf.json",
    "docker:build": "docker build -t nexus-debug-assistant .",
    "docker:run": "docker run -p 3010:3010 nexus-debug-assistant"
  },
