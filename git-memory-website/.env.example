# Google Analytics Configuration
# Get your GA4 Measurement ID from Google Analytics
# Format: G-XXXXXXXXXX
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# Mixpanel Configuration (Optional)
# Get your project token from Mixpanel dashboard
NEXT_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_token_here

# Environment
NODE_ENV=development

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_MIXPANEL=false

# Contact Form Configuration
NEXT_PUBLIC_CONTACT_EMAIL=info@comdee.co.th

# Social Media Links
NEXT_PUBLIC_GITHUB_URL=https://github.com/gitmemory
NEXT_PUBLIC_TWITTER_URL=https://twitter.com/gitmemory
NEXT_PUBLIC_LINKEDIN_URL=https://linkedin.com/company/gitmemory