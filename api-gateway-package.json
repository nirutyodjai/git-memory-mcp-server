  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "api-gateway-*.js",
      "!api-gateway-*.test.js",
      "!api-gateway-*.spec.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-console": "off",
      "no-process-exit": "off",
      "global-require": "off",
      "import/no-dynamic-require": "off"
    },
    "env": {
      "node": true,
      "es2022": true,
      "jest": true
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 100,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "nodemonConfig": {
    "watch": [
      "api-gateway-*.js",
      "*.json"
    ],
    "ignore": [
      "node_modules/",
      "coverage/",
      "logs/",
      "cache/",
      "tmp/"
    ],
    "ext": "js,json",
    "env": {
      "NODE_ENV": "development"
    }
  },
  "nyc": {
    "include": [
      "api-gateway-*.js"
    ],
    "exclude": [
      "**/*.test.js",
      "**/*.spec.js",
      "coverage/**"
    ],
    "reporter": [
      "text",
      "lcov",
      "html"
    ],
    "check-coverage": true,
    "lines": 80,
    "functions": 80,
    "branches": 80,
    "statements": 80
  }
}