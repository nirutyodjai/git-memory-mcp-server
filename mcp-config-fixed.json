{
  "mcpServers": {
    "figma-developer-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "figma-developer-mcp",
        "--figma-api-key=YOUR_FIGMA_TOKEN_HERE",
        "--stdio"
      ],
      "env": {
        "MCP_SERVER_NAME": "Figma Developer MCP - Design Context Provider"
      }
    },
    "3d-sco-playwright": {
      "command": "node",
      "args": [
        "d:\\3D-SCO\\scripts\\mcp-playwright-server.js"
