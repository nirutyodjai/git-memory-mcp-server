    "ClickUp": {
      "command": "npx",
      "args": [
        "-y",
        "@taazkareem/clickup-mcp-server@latest",
        "--env",
        "CLICKUP_API_KEY=your_api_key_here",
        "--env",
        "CLICKUP_TEAM_ID=your_team_id_here"
      ],
      "env": {
        "CLICKUP_API_KEY": "",
        "CLICKUP_TEAM_ID": ""
      },
      "fromGalleryId": "windalfin.clickup-mcp-server"
    }
  },
  "globalSettings": {
    "logLevel": "info",
    "timeout": 30000,
    "retries": 3,
    "rateLimit": {
      "enabled": true,
      "windowMs": 900000,
      "maxRequests": 100
    }
  },
  "traeIntegration": {
    "enabled": true,
    "version": "1.0.0",
    "compatibility": "trae-ai-v4",
    "features": [
      "browser-automation",
      "web-scraping",
      "3d-modeling",
      "sequential-thinking",
      "memory-management"
    ],
    "endpoints": {
      "playwright": "/api/mcp/playwright",
      "multifetch": "/api/mcp/fetch",
      "blender": "/api/mcp/blender",
      "thinking": "/api/mcp/thinking",
      "memory": "/api/mcp/memory"
    },
    "baseUrl": "http://localhost:3001"
  }
}